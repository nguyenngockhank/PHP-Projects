RewriteEngine on
#RewriteBase /

RewriteRule ^([a-zA-Z0-9_-]+)/$ index.php?link=$1 [L] 
RewriteRule ^([a-zA-Z0-9_-]+)$ index.php?link=$1 [L] 
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/$ index.php?link=$1/$2 [L] 
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)$ index.php?link=$1/$2 [L] 
RewriteRule ^([a-zA-Z0-9_-]+)/(.*)/([a-zA-Z0-9_-]+)/$ index.php?link=$1/$2/$3 [L] 
RewriteRule ^([a-zA-Z0-9_-]+)/(.*)/([a-zA-Z0-9_-]+)$ index.php?link=$1/$2/$3 [L] 



